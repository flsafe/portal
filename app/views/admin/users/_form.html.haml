- puts target_path
= form_for target_path, :html => { multipart: true, :class => 'form-horizontal' } do |f|
  - if @staff.errors.any? 
    .errorExplaination.alert.alert-danger
      %p
        = "#{pluralize(@staff.errors.count, 'error')} prevented your profile from being saved."
      %ul
        - @staff.errors.full_messages.each do |msg|
          %li= msg
  = render partial: 'form_fields', locals: {f: f}
  = f.submit "Save", class: 'btn btn-primary'